} else {\r\n      const data = axios\r\n        .get(\"http://localhost:8000/users\", {\r\n          headers: {\r\n            \"x-access-token\": authentication.store,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          setMember(res.data);\r\n        });\r\n      console.log(authentication);\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <div className={styles.teammember}>\r\n      <div className={styles.teammemberlist}>\r\n        <h1 className={styles.heading}>Team Members</h1>\r\n      </div>\r\n      <div className={styles.teammemberscontainer}>\r\n        {members.map((member) => (\r\n          <div key={member._id}>\r\n            <CardComp\r\n              firstname={member.first_name}\r\n              lastname={member.last_name}\r\n              empid={member.emp_id}\r\n              role={member.role}\r\n              id={member._id}\r\n              emailid={member.email}\r\n              technologies={member.technologies}\r\n              projects={member.projects}\r\n            ></CardComp>\r\n          </div>\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default TeamMember;\r\n","C:\\Users\\nkhan\\Documents\\PERSONAL\\Learnings\\React\\MF Info App\\mf_info\\src\\components\\technology.js",["144","145"],"import React, { useEffect, useRef, useState } from \"react\";\r\nimport axios from \"axios\";\r\nimport ModalForm from \"./modal\";\r\nimport { Player } from \"@lottiefiles/react-lottie-player\";\r\n\r\nimport { generateErrorToast, generateSuccessToast } from \"../utils/toast/index\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport styles from \"../css/technologies.module.css\";\r\nconst Technology = () => {\r\n  let modalRef = useRef();\r\n  const history = useHistory();\r\n  const [techdisplay, settechdisplay] = useState({\r\n    techname: \"\",\r\n    usedfor: \"\",\r\n  });\r\n  const authentication = JSON.parse(localStorage.getItem(\"login\"));\r\n  const [tech, setTech] = useState([]);\